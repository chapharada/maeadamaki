{
  "name": "development",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "run-s build:*",
    "init:img": "node ./module/makeImg init",
    "build:pug": "node ./module/makePug init",
    "write:sass": "sass \"./src/assets/sass:src/assets/css\" --no-source-map",
    "build:js": "node ./module/makeCopy init js",
    "watches": "run-p watches:*",
    "watches:pug": "chokidar \"./src/pug/**/*.pug\" -i \"./src/**/_*.pug\" -i \"./src/**/*copy.pug\"  -i \"./src/pug/admin/*.*\" -c \"node ./module/makePug watch {path} {event}\"",
    "watches:layout": "chokidar \"./src/layout/**/*.pug\" -i \"./src/layout/**/*copy.pug\" -c \"node ./module/makePug init\"",
    "watches:var": "chokidar \"./src/var/**/*.pug\"  -i \"./src/var/**/*copy.pug\" -c \"node ./module/makePug init\"",
    "watches:include": "chokidar \"./src/include/**/*.pug\"  -i \"./src/include/**/*copy.pug\" -c \"node ./module/makePug init\"",
    "watches:router": "chokidar \"./router/**/*.json\" -i \"./router/**/_*.json\" -i \"./router/**/*copy.json\" -c \"node ./module/makePug router {path} {event}\"",
    "watches:img": "node \"./module/makeImg.js\" watch",
    "watches:sass": "chokidar \"./src/assets/sass/**/*.{scss,css}\" -i \"./src/assets/sass/**/_*.{scss,css}\" -i \"./src/assets/sass/**/*copy.{scss,css}\" -c \"node ./module/makeSass watch {path} {event}\"",
    "watches:_sass": "chokidar \"./src/assets/sass/component/_*.scss\" \"./src/assets/sass/foundation/_*.scss\" \"./src/assets/sass/global/_*.scss\" \"./src/assets/sass/layout/_*.scss\" -c \"node ./module/makeSass init\"",
    "watches:js": "webpack -w --mode development --node-env production",
    "watches:server": "browser-sync start -s s3 -w s3/**/*.html s3/assets/css/**/*.css s3/assets/css/js/*.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "autoprefixer": "^10.3.2",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.5",
    "chokidar": "^3.5.2",
    "chokidar-cli": "^3.0.0",
    "fs": "0.0.1-security",
    "glob": "^7.1.7",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.38.0",
    "sharp": "^0.29.0",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Android >= 4",
    "iOS >= 9"
  ]
}
